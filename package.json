    "husky": "^4.2.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup-plugin-postcss": "^3.1.1",
    "tsdx": "^0.13.2",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@reach/auto-id": "^0.10.1",
    "@reach/observe-rect": "^1.1.0",
    "@reach/popover": "^0.10.1",
    "axe-core": "^3.5.3"
  },
  "eslintConfig": {
